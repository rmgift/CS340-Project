<h3>This will show the current table of directors</h3>
<table>
    <caption>List of Directors</caption>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>cid</th>
    </tr>
    {{#if results}}
    {{#each results}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.first_name}}</td>
        <td>{{this.last_name}}</td>
        <td>{{this.age}}</td>
        <td>{{this.cid}}</td>
    </tr>
    {{/each}}
    {{/if}}
</table>
